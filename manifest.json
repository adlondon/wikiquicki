{
  "manifest_version": 2,

  "name": "WikiQuicki",
  "description": "This extension gives a quick snippet of a Wikipedia article",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://en.wikipedia.org/"
  ]
}
